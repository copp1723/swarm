{
  "name": "mcp-agent-chat-frontend",
  "version": "1.0.0",
  "description": "Frontend JavaScript modules for MCP Agent Chat Interface",
  "type": "module",
  "scripts": {
    "test": "jest --config tests/jest.config.js",
    "test:watch": "jest --config tests/jest.config.js --watch",
    "test:coverage": "jest --config tests/jest.config.js --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --config tests/jest.config.js --runInBand"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "keywords": [
    "javascript",
    "es6",
    "modules",
    "agents",
    "chat",
    "websocket",
    "mcp"
  ],
  "author": "MCP Development Team",
  "license": "MIT"
}

